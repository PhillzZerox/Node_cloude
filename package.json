function NumberSplit(num){
    var num2 = num/2
    if (num2 > -1){
        console.log("[", Math.floor(num2), ",",Math.ceil(num2), "]");
    }
    else{
        console.log("[", (num2-0.5), ",",(num2+0.5),"]");
    }
    return '';
}
console.log(NumberSplit(4));
console.log(NumberSplit(10));
console.log(NumberSplit(11));
console.log(NumberSplit(-9));
